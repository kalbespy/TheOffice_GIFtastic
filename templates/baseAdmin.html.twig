<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link
		rel="icon" type="image/png" href="{{ asset('build/images/prison-mike.png') }}"/>
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>

	<body class="min-vh-100">
		{% include "components/_navbar.html.twig" %}

		<div class="container-fluid d-md-flex p-0 min-vh-100">
			<div class="me-5">
				{% include "components/_navBarAdmin.html.twig" %}
			</div>
			<div class="col-9"> {% block body %}{% endblock %}
				</div>
			</div>
			{% include "components/_footer.html.twig" %}

		</body>
	</html>
